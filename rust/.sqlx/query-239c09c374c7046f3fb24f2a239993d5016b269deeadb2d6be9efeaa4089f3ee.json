  "query": "\n        SELECT\n            de.tick,\n            de.message_type,\n            de.message,\n            de.player_id as \"small_id: i16\",\n            de.gold_amount,\n            ply.client_id,\n            ply.name\n        FROM\n            analysis_1.display_events de\n            JOIN analysis_1.players ply\n                ON de.game_id = ply.game_id\n                AND de.player_id = ply.small_id\n        WHERE\n            de.game_id = $1::char(8)\n        ",
